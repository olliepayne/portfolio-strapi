{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "mainImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::skill.skill",
      "inversedBy": "projects"
    },
    "summary": {
      "type": "text",
      "maxLength": 250,
      "required": true
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "basic.seo",
      "required": true
    },
    "content": {
      "type": "richtext"
    },
    "company": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::company.company"
    },
    "duration": {
      "type": "string",
      "required": true
    }
  }
}
